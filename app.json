{
  "expo": {
    "name": "FoodTrack",
    "slug": "FoodTrack",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.therefridgenators.foodtrack",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.therefridgenators.foodtrack",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    }
  }
}
